    "h=int(input( \"veuillez saisir le nombres d'heures:\"))\n",
    "p=int(input( \"veuillez saisir le prix initiale d'une heure:\"))\n",
    "h1=39\n",
    "h2=h-39\n",
    "h3=h-44\n",
    "h4=h-49\n",
    "if 0<h<=39:\n",
    "    print (\"votre montant est : \", p*h ,\"dh\")\n",
    "elif 40<=h<=44 :\n",
    "    p=(p*h1)+((p*1.5)*h2)\n",
    "    print(\"votre montant est : \", p ,\"dh\")\n",
    "elif 45<=h<=49:\n",
    "    p=(p*h1)+((p*1.5)*5)+((p*1.75)*h3)\n",
    "    print(\"votre montant est : \", p ,\"dh\")\n",
    "elif 50<=h :\n",
    "    p=(p*h1)+((p*5)*1.5)+((p*5)*1.75)+((p*h4)*2)\n",
    "    print(\"votre montant est : \", p ,\"dh\")\n",
    "else :\n",
    "    print(\"erreur\")\n",
    "\n",
    "\n",
    "\n",
